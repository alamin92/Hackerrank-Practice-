#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    int m[n];
    
    for(int i=0; i<n; i++){
        cin>>m[i];
    }
    for(int i=n; i>0; i--){
        cout<<m[i-1]<<" ";
    }
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
